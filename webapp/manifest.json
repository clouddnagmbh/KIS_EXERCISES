{
	"_version": "1.12.0",
	"sap.app": {
		"id": "at.clouddna.training00.FioriDeepDive",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZHOFIO_SRV": {
				"uri": "/sap/opu/odata/sap/ZHOFIO_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "at.clouddna.training00.FioriDeepDive.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "at.clouddna.training00.FioriDeepDive.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request",
					"useBatch": true
				},
				"dataSource": "ZHOFIO_SRV",
				"preload": true
			},
			"genderModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "data/genders.json",
				"preload": false
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "at.clouddna.training00.FioriDeepDive.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [{
				"pattern": "RouteApp",
				"target": ["TargetApp"]
			}, {
				"name": "Main",
				"pattern": "",
				"titleTarget": "",
				"greedy": false,
				"target": ["Main"]
			}, {
				"name": "Customer",
				"pattern": "customer/{customerid}",
				"titleTarget": "",
				"greedy": false,
				"target": ["Customer"]
			}],
			"targets": {
				"TargetApp": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "App",
					"viewName": "App"
				},
				"Customer": {
					"viewType": "XML",
					"viewName": "Customer"
				},
				"Main": {
					"viewType": "XML",
					"viewName": "Main"
				}
			}
		}
	}
}